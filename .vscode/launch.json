    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "SOURCE_GITHUB_TOKEN": "",
                "DESTINATION_GITHUB_TOKEN": ""
            },
            "args": ["api.github.com", "api.github.com", "mano-python", "mano-python", "testrepo3", "testrepo", "main", "dev5"]
        }
    ]
}